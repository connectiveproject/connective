{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cd client && vue-cli-service serve",
    "build": "cd client && vue-cli-service build",
    "test:unit": "cd client && vue-cli-service test:unit",
    "test:debug": "cd client && node --inspect-brk node_modules/.bin/vue-cli-service test:unit --no-cache --watch --runInBand",
    "lint": "cd client && vue-cli-service lint",
    "i18n:report": "cd client && vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "imagemin": "cd client && node imagemin.js",
    "json-server": "cd client && json-server && ./run.sh",
    "storybook": "cd client && start-storybook -p 6006",
    "build-storybook": "cd client && build-storybook",
    "cy:open": "cypress open"
  },
